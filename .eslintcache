}/>\n        }\n      })\n\n    function handleCategoryClick(event){\n        setUpdateFav(event.target.value)\n    }\n\n\n    function handleUserSubmit(event){\n        event.preventDefault();\n\n        const data = {favorite: updatedFav}\n        console.log(data, \"this is the data\")\n\n        fetch(`http://localhost:3000/api/v1/users/${currentUser.id}`, {\n            method: 'PATCH',\n            headers: {\n                'Content-Type': 'application/json',\n            },\n            body: JSON.stringify(data),\n            })\n        .then((r) => r.json())\n        .then((r) => reloadCurrentUser(currentUser))\n    }\n\n    function reloadCurrentUser(currentUser) {\n        fetch(`http://localhost:3000/api/v1/users/${currentUser.id}`)\n            .then((r) => r.json())\n            .then(r => setCurrentUser(r));\n    }\n\n    return (\n        <div>\n        <h1>{currentUser.name}</h1>\n        <h2>Favorite Genre: {currentUser.favorite}</h2>\n\n            <select onChange={handleCategoryClick} name=\"filter\">\n                    <option value=\"All\">Change Favorite?</option>\n                    <option value=\"All\"> All </option>\n                    <option value=\"Hip-Hop\"> Hip-Hop </option>\n                    <option value=\"Rock\"> Rock </option>\n                    <option value=\"Pop\"> Pop </option>\n                    <option value=\"Synth-Pop\"> Synth-Pop </option>\n                    <option value=\"EDM\"> EDM </option>\n                    <option value=\"Party\"> Fight Ian </option>\n            </select>\n\n            <button onClick={handleUserSubmit} className=\"cardbutton\">Update Favorite</button>\n\n        <h2>Here is the list of events you plan on going to!</h2>\n        {currentUser.tickets ? <div> <UserEventList events={userEvents} tickets={userTickets} setTickects={setTickects} currentUser={currentUser} setCurrentUser={setCurrentUser}/> </div>: null}\n\n        <h3>Recommended events for {currentUser.name}:</h3>\n        \n        <div className=\"slider-menu\">\n            {recEventsMapped}\n        </div>\n\n        </div>\n\n    )\n}\nexport default UserShow;","/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/Event.js",[],"/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/HomeEventCard.js",[],"/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/Venue.js",[],"/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/RecSlider.js",["117"],"import React from \"react\"\nimport { Link } from \"react-router-dom\"\n\nfunction RecSlider({event}){\n\n\n    return(\n\n        <Link to={`/events/${event.id}`}>\n        <div className=\"card\">\n                <div className=\"outer\">\n                    <div>\n                        <img src={event.image} />\n                    </div>\n                    <div>\n                        <p><strong>{event.performer_name}</strong></p>\n                    </div>\n                </div>\n        </div>\n        </Link>\n    )\n\n}\n\nexport default RecSlider","/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/pages/UserEventList.js",[],"/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/pages/UserEventRender.js",[],"/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/Test.js",["118"],"import React from \"react\"\n\n\n\nfunction Test({}){\n\n    return(\n        <div>\n        \n      <tr>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n      </tr>\n      <tr>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n      </tr>\n      <tr>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n          <td>Button</td>\n      </tr>\n      <tr>\n          <td>Bu