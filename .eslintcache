>\n        <h1>{event.event_name}</h1>\n        <h2>{event.performer_name}</h2>\n        <img className=\"imgShow\" src={event.image} />\n        <img className=\"imgShow\" src={\"venue-frontend/public/images/patrick.png\"} />\n        <br></br>\n        <p>About: {event.description}</p>\n        <button onClick={handleBuyButton}>Buy Ticket!</button>\n        </div>\n    )\n}\nexport default EventShow;","/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/pages/LoginButtons.js",["70","71"],"import React, {useEffect, useState} from \"react\";\nimport { Link } from \"react-router-dom\"\n\nfunction LoginButtons({currentUser, setCurrentUser, setUserTicketList}) {\n\n    // auto-login\n    useEffect(() => {\n      fetch(\"http://localhost:3000/autologin\")\n        .then((r) => r.json())\n        .then(r => setCurrentUser(r));\n    }, []);\n  \n    // manual login\n    function handleLogin() {\n      fetch(\"http://localhost:3000/autologin\")\n        .then((r) => r.json())\n        .then(setCurrentUser);\n    }\n  \n    // manual logout\n    function handleLogout() {\n      setCurrentUser(null);\n    }\n\n    //  <button onClick={handleLogout}><Link to={`/`}>Log out</Link></button>\n    //<button onClick={handleLogin}>Log in</button>\n\n    // <Link to={`/events/${event.id}`}>{event.event_name}</Link>\n\n    return (\n      <div>\n        {currentUser ? <button className=\"button\" onClick={handleLogout}><Link to={`/`}>Log out</Link></button> : <button className=\"button\" onClick={handleLogin}>Log in</button> }\n        {currentUser ?  null : <button className=\"button\"><Link to={`/sign-up`} >Sign Up</Link></button> }\n        {currentUser ? <h1>Welcome, <Link to={`/users/${currentUser.id}`}>{currentUser.name}</Link></h1> : null }\n      </div>\n    );\n  }\n\nexport default LoginButtons;\n\n\n","/Users/kevinbotero/Flatiron/react/venue_app/venue-frontend/src/Event.js",["72"],"import React from \"react\"\nimport { Link } from \"react-router-dom\"\n\n//<h2><L